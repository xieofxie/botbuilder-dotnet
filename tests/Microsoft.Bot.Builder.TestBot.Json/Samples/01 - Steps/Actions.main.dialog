{
    "$schema": "../../testbot.schema",
    "$kind": "Microsoft.AdaptiveDialog",
    "triggers": [
        {
            "$kind": "Microsoft.OnBeginDialog",
            "actions": [
                {
                    "$kind": "Microsoft.SendActivity",
                    "activity": "${settings.array}"
                },
                {
                    "$kind": "Microsoft.SendActivity",
                    "activity": "${settings.object}"
                },
                {
                    "$kind": "Microsoft.SendActivity",
                    "activity": "${settings.structure}"
                },
                {
                    "$kind": "Microsoft.SendActivity",
                    "activity": "${settings.array['1']}"
                },
                {
                    "$kind": "Microsoft.SendActivity",
                    "activity": "${settings.object['123']}"
                },
                {
                    "$kind": "Microsoft.SetProperty",
                    "property": "turn.data",
                    "value": "=json({'123':'234'})"
                },
                {
                    "$kind": "Microsoft.SendActivity",
                    "activity": "${turn.data}"
                }
            ]
        }
    ]
}